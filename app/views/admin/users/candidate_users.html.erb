<h1>Candidate Users</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Invitation code</th>
      <th>User</th>
      <th>Phone</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.invitation_code %></td>
        <td><%= user.user_id %></td>
        <td><%= user.phone %></td>
        <td><%= user.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
